{
    "name": "Laravolt/Support",
    "description": "Laravolt support",
    "keywords": [
        "laravolt",
        "support",
        "pagination"
    ],
    "homepage": "https://github.com/laravolt/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Support\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
